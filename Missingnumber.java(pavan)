   import java.util.*;

   public class Missingnumber{
   public static void main(String[] args) {

   int sum = 0 ;

   Scanner sc = new Scanner(System.in);
   System.out.println("enter the value of n");
   int n = sc.nextInt();
   System.out.println("enter " + ( n-1 ) + " elements of array");
   int[] b = new int[n-1];

   for(int i=0;i<b.length;i++){
   b[i] = sc.nextInt();
   sum = sum+b[i];
   }

   int sumofnatnum= n*(n+1)/2;
   System.out.println("The missing number ="+ ( sumofnatnum - sum));
   }
   }
